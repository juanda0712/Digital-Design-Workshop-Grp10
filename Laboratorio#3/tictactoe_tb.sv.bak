module tictactoe_tb();

    logic clk;
    logic rst;
    logic select;
	 logic move;
	 logic attack;
    logic orden;
    logic cant_player;
    logic [1:0] board [3:1][3:1];
    logic [1:0] winner;

	tictactoe game(
				 .clk(clk),
				 .rst(rst),
				 .select(select),
				 .move(move),
				 .attack(attack),
				 .orden(orden),
				 .cant_player(cant_player),
				 .board(board),
				 .winner(winner));

endmodule